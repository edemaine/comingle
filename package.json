    "@use-it/event-listener": "0.1.7",
    "@use-it/interval": "1.0.0",
    "body-parser": "1.19.1",
    "bootstrap": "4.6.0",
    "flexlayout-react": "0.5.12",
    "katex": "0.15.2",
    "markdown-it": "12.3.2",
    "markdown-it-texmath": "0.9.7",
    "meteor-node-stubs": "1.1.0",
